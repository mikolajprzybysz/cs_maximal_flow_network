<Window x:Class="MaximalFlowNetwork.VertexAdd"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Please type new vertex name" Height="106" Width="240" ResizeMode="NoResize">
    <Grid>
        <StackPanel Name="stackPanel1" Orientation="Horizontal" Margin="16,10,12,12" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox Margin="4,0,0,0" Height="23" Name="nameTxt" Width="63"></TextBox>
            <Button Name="okButton" Margin="4,0,0,0" Height="23" Width="50">Ok</Button>
            <Button Height="23" Margin="4,0,0,0" Name="cancelButton" Width="50">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
