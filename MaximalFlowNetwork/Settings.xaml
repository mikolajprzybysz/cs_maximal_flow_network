<Window x:Class="MaximalFlowNetwork.Settings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Settings" Height="140" Width="179" ResizeMode="NoResize">
    <Grid>
        <ComboBox Height="23" Margin="78,12,0,0" Name="comboBoxSource" VerticalAlignment="Top" HorizontalAlignment="Left" Width="58" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="78,41,0,0" Name="comboBoxSink" VerticalAlignment="Top" Width="58" />
        <Label Height="21" HorizontalAlignment="Left" Margin="12,14,0,0" Name="label1" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Right">Source</Label>
        <Label Height="21" HorizontalAlignment="Left" Margin="12,41,0,0" Name="label2" VerticalAlignment="Top" Width="60" HorizontalContentAlignment="Right">Sink</Label>
        <Button Height="23" Margin="12,0,0,11" Name="buttonOk" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="48">Ok</Button>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,12,9" Name="buttonCancel" VerticalAlignment="Bottom" Width="48">Cancel</Button>
    </Grid>
</Window>
